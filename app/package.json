{
  "name": "taedal-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": { "node": "20.x" },
  "scripts": {
    "preinstall": "git lfs install || true",
    "dev": "vite",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "make:panel": "node scripts/make-panel-glb.mjs 0.01",
    "vercel-build": "git lfs install && git -C .. lfs fetch --include=\"app/public/3d/*\" --exclude=\"\" https://github.com/ndstrzz/taedal-v7.git && git -C .. lfs checkout --include=\"app/public/3d/*\" && npm install --no-audit --progress=false && npm run build"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.12.10",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@google/model-viewer": "^4.1.0",
    "@hookform/resolvers": "^5.2.2",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@supabase/supabase-js": "^2.75.0",
    "@tailwindcss/cli": "^4.1.14",
    "@tanstack/react-query": "^5.90.3",
    "@walletconnect/ethereum-provider": "^2.22.4",
    "driver.js": "^1.3.6",
    "ethers": "^6.15.0",
    "ogl": "^1.0.11",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-easy-crop": "^5.5.3",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.9.4",
    "three": "^0.172.0",
    "three-stdlib": "^2.36.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@nomicfoundation/hardhat-ethers": "^4.0.2",
    "@openzeppelin/contracts": "^5.4.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/three": "^0.172.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.6.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "hardhat": "^3.0.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  },
  "overrides": {
    "three": "^0.172.0"
  }
}
